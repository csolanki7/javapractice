class box {
	double width;
	double height;
	double depth;
}



class boxdemo {
	public static void main (String args[]){
		box mybox = new box();
		box mybox2 = new box ();
		
		
		double vol;
		
		mybox.width = 10;
		mybox.height= 20;
		mybox.depth= 15;
		
		mybox2.width=3;
		mybox2.height=6;
		mybox2.depth=9;
		
		
		
		vol=mybox.width*mybox.height*mybox.depth;
		
		System.out.println("volume is : " +vol);
		
		vol=mybox2.width*mybox2.height*mybox2.depth;
		System.out.println("volume is : " +vol);
	}
}

output :
volume is : 3000.0
volume is : 162.0
