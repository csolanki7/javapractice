class box {
	double width;
	double height;
	double depth;  

double volume () {
	
	return width * height * depth ; 
}
void setDim(double w, double h, double d ){
	width = w;
	height = h;
	depth = d;
	
}
}

class boxdemo {
	public static void main (String args[]){
		box mybox = new box();
		box mybox2 = new box ();
	     double vol;
	     
	     
		
		mybox.setDim(10,20,15);
		mybox2.setDim(3, 6, 9);
		
		vol = mybox.volume();
		System.out.println("volume is " +vol);
		
		vol = mybox2.volume();
		System.out.println("volume is " +vol);
		
		}
}	
	
	output : 
	volume is 3000.0
volume is 162.0

	
		
		
		
