package first_cons;

/**
 *
 * @author csolanki7
 */
public class First_cons {

    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) {
        // TODO code application logic here
        
        int[] A = {1,2,3,4,5,6,7,8,9};
        
        int Sum = 0; 
        
        for (int N : A) {
            
            Sum = Sum + N;
            
        }
        
        System.out.println("sum of array is elemets is " +Sum);   
    } 
}
