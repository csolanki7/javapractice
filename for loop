package javatest.pkg1;

/**
 *
 * @author Chirag007
 */
public class Javatest1 {
    
  public static void main (String args[]){
   
      int x;
      
      for (x=0; x<=3 ; x++) {
          
          int y = -1;
          
      System.out.println(" y is : " +y );
      
      y = 100;
      
      System.out.println("y is : " +y);
      
      
      }
  }
 }
 
 output :
 
 y is : -1
y is : 100
 y is : -1
y is : 100
 y is : -1
y is : 100
     
    
    

